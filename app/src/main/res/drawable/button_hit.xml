<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <gradient
                android:startColor="@color/green1"
                android:centerColor="@color/brown"
                android:endColor="#291A04"
                android:type="linear"
                android:angle="10"/>
            <stroke
                android:width="14px"
                android:color="#000000" />

            <corners
                android:topLeftRadius="430dp"
                android:bottomLeftRadius="430dp"/>
        </shape>
    </item>

    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#80FFFFFF" />
            <stroke
                android:width="14px"
                android:color="#858585" />
            <corners
                android:topLeftRadius="430dp"
                android:bottomLeftRadius="430dp"/>
        </shape>
    </item>
</selector>